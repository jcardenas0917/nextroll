    "eslint": "^6.6.0",
    "nodemon": "^1.19.4"
  },
  "homepage": "https://github.com/jcardenas0917/mynextroll#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "moment": "^2.24.0",
    "momentjs": "^2.0.0",
